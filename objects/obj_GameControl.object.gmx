            <string>var event_id = ds_map_find_value(async_load, "id");

if (global.server == event_id ){
    //Não faz nada pois isso é trabalho do servidor
} else if(event_id == global.client){
    if (!global.have_server){
        receiving_data = true;
        buff = ds_map_find_value(async_load, "buffer");
        buffer_seek(buff, buffer_seek_start, 0);
        switch (buffer_read(buff, buffer_s16)){
            case ENEMY_PIECES_POSITION:
                scr_PositionateEnemyPieces();
                break;
            case PIECES_POSITION:
                scr_PositionatePieces();
                break;
            case RESTART:
                room_restart();
                break;
            case BACK:
                room_goto_previous();
                break;
        }
        receiving_data = false;
    }
} else{
    //Recebeu dados de um cliente externo
    receiving_data = true;
    var buff = ds_map_find_value(async_load, "buffer");
    buffer_seek(buff, buffer_seek_start, 0);
    switch (buffer_read(buff, buffer_s16)){
        case ENEMY_PIECES_POSITION:
            scr_PositionateEnemyPieces();
            break;
        case PIECES_POSITION:
            scr_PositionatePieces();
            break;
        case RESTART:
            room_restart();
            break;
        case BACK:
            room_goto_previous();
            break;
    }
    receiving_data = false;
}
</string>
